package Ch8;

public class MyLinkedList {
	
	private Node head = null; //head is a reference
	
	//innerClass
	private class Node{
		private String data;
		private Node link;
		
		public Node(String data) { //Node class' constructor
			this.data = data;
		}
	}
	
	//method1
	public void add(String data) {
		Node newNode = new Node(data);
		if(head==null) {
			head = newNode;
		}
		else
		{
			Node next = head;
		}
		
		while()
	}
	
	//method2
}
