package Ch12;

class NewBag<T,N>{ // T ~ class
	private T thing;
	private N name;
	
	public NewBag() {}
	
	public NewBag(T thing) {
		this.thing = thing;
	}

	public T getThing() {
		return thing;
	}

	public void setThing(T thing) {
		this.thing = thing;
	}
	
	public N getName() {
		return name;
	}

	public void setName(N name) {
		this.name = name;
	}
	
	void showType() {
		System.out.println("a type of T is : " + thing.getClass().getName());
		System.out.println("a type of N is : " + name.getClass().getName());
	}
}

class Newbook{

	@Override
	public String toString() {
		return "Newbook";
	}
	
}
class Newpencilcase{}
class Newnotebook{}

public class Bagpack3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		NewBag<book, String> bag1 = new NewBag<>();
		bag1.setThing(new book());
		NewBag<Newpencilcase> bag2 = new NewBag<>(new Newpencilcase());
		NewBag<Newnotebook> bag3 = new NewBag<>(new Newnotebook());
		
		bag1.showType();
		bag2.showType();
		bag3.showType();
		
	}

}
