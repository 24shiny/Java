package Ch10;

import java.io.FileInputStream;

public class Test04 {
	
	public static void test() {
	try {
		try(FileInputStream fi = new FileInputStream("hello.txt")){
		int c=fi.read();
		System.out.println((char)c);
	}
	catch(Exception e){
		e.getMessage();
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		test();
		}
		
	}
}
}