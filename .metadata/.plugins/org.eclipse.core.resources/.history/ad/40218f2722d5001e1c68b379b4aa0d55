package Ch10;

public class GG {
		// TODO Auto-generated method stub
		public void print(int i) {
			System.out.println(i + "ë‹¨");
			for(int j=1 ; j<=9; j++) {
				if(j==9) {
					System.out.println(i + "*" + j + "=" + i*j);
				}
			}
		}
		
		public void printall() {
			for(int i=1; i<10; i++) {
				for(int j=2; j<10; j++) {
					System.out.print(i + "*" + j + "=" + i*j + "\t");
				}
					System.out.println();
			}
		}
		
		public void printall2() {
			for(int i=0; i<3; i++) {
				for(int j=2; j <5; j++) {
					print(3*i+j);
				}
				
			}
			System.out.println();
		}
		
		
}
