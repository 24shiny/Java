package Ch12;

class Bag<T>{
	private T thing;
	
	public Bag(T thing) {
		this.thing = thing;
	}

	public T getThing() {
		return thing;
	}

	public void setThing(T thing) {
		this.thing = thing;
	}
	
	void showType() {
		System.out.println("a type of T is : " + thing.getClass().getName());
	}
}

public class Bagpack {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
